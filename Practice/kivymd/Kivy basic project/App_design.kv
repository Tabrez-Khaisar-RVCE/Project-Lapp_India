<AddComponentPartNumber>:
    on_pre_enter:root.PNO()

    BoxLayout:
        orientation:'vertical'
        canvas.before:
            Color:
                rgba:(1,1,1,1)
            Rectangle:
                size:self.size
                pos:self.pos
        BackButtonLayout:
        BoxLayout:
            orientation:'vertical'
            spacing:'10dp'
            padding:'10dp'
            canvas.before:
                Color:
                    rgba:(1, 196/255, 196/255,0.7)
                Rectangle:
                    size:self.size
                    pos:self.pos
            GridLayout:
                cols:3
                # pos_hint:{'x':0,'top':0.95}
                spacing:('30dp','5dp')
                size_hint:None,None
                size:'750dp','100dp'

                HolderLabel:
                    size_hint_x:None
                    width:'150dp'
                    text:'Part Number'
                EmptyLabel:
                    id:PartNumber
                    size_hint_x:None
                    width:'200dp'
                    text:''
                Label:
                    text:''

                HolderLabel:
                    size_hint_x:None
                    width:'150dp'
                    text:'Component QTY'
                EmptyLabel:
                    id:ComponentQTY_txt
                    size_hint_x:None
                    width:'200dp'
                    text:''
                Label:
                    text:''

                HolderLabel:
                    size_hint_x:None
                    width:'150dp'
                    text:'Component PNO'
                FixedTextInput:
                    id:ComponentPNO
                    size_hint_x:None
                    width:'200dp'
                    text:''
                BoxLayout:
                    spacing:'5dp'
                    SelectionButton:
                        size_hint_x:None
                        width:'150dp'
                        text:'Insert'
                        on_release:Insert_data.ids.CI_Cutting_table_Input_screen.Comp_CI(root.Component_details_values()) if root.validate_PNO() else root.validate_PNO_state()
                    ControlledLabel:
                        id:err_label
                        text:''
                        color:(1,0,0,1)
                        halign:'left'

            BoxLayout:
                canvas.before:
                    Color:
                        rgba:(0, 1, 1,0.7)
                    Rectangle:
                        size:self.size
                        pos:self.pos
                Tab_In:
                    id:Insert_data
<Tab_In>:
    background_image:""
    tab_height:'30dp'
    tab_width:'140dp'
    canvas.before:
        Color:
            rgba:(1.2, 100/255, 50/255, 0.7)
        Rectangle:
            size:self.size
            pos:self.pos

    do_default_tab:False
    InfoPanelItem:
        id:PNO_Cutting
        text:'Cutting'
        background_color:(2.5,1,0.5,1)
        on_release:PNO_Cutting.background_color=(2.5,1,0.5,1);PNO_Crimping.background_color=(1,1,1,0); PNO_Assembly.background_color=(1,1,1,0)
        Cutting_table_Input:
            id:CI_Cutting_table_Input_screen

    InfoPanelItem:
        id:PNO_Crimping
        text:'Crimping'
        on_release:PNO_Crimping.background_color=(2.5,1,0.5,1);PNO_Cutting.background_color=(1,1,1,0); PNO_Assembly.background_color=(1,1,1,0);CRI_Crimping_table_Input_screen.CI_CRI_Info(CI_Cutting_table_Input_screen.CI_CRI_Data_Sending())
        Crimping_table_Input:
            id:CRI_Crimping_table_Input_screen

    InfoPanelItem:
        id:PNO_Assembly
        text:'Assembly'
        on_release:PNO_Assembly.background_color=(2.5,1,0.5,1);PNO_Cutting.background_color=(1,1,1,0); PNO_Crimping.background_color=(1,1,1,0); ASM_Assembly_table_Input_screen.CI_ASM_details(CI_Cutting_table_Input_screen.CI_ASM_Data_Sending())
        Assembly_table_Input:
            id:ASM_Assembly_table_Input_screen

